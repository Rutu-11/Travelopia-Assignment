{
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!your-module-to-include)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.test.js",
      "<rootDir>/src/__tests__/**/*.test.jsx"
    ]
  }
  